<div id="<%= dom_id link %>">
  <p>
    <strong>Short url:</strong>
    <%= @complete_slug %>
  </p>

  <p>
    <strong>Url destino:</strong>
    <%= link.url %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= link.name %>
  </p>

  <p>
    <strong>Expires at:</strong>
    <%= link.expires_at %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= link.password %>
  </p>

  <p>
    <strong>Remaining accesses:</strong>
    <%= link.remaining_accesses %>
  </p>

  <p>
    <strong>Link type:</strong>
    <%= link.link_type %>
  </p>

</div>
